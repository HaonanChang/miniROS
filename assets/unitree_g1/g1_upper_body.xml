  <actuator>
    <position name="torso_x_joint" joint="torso_x" ctrlrange="-1.0 1.0" kp="100" kv="20"/>
    <position name="torso_y_joint" joint="torso_y" ctrlrange="-1.0 1.0" kp="100" kv="20"/>
    <position name="torso_z_joint" joint="torso_z" ctrlrange="-1.0 1.0" kp="100" kv="20"/>
    <position name="torso_roll_joint" joint="torso_roll" ctrlrange="-100 100" kp="100" kv="10"/>
